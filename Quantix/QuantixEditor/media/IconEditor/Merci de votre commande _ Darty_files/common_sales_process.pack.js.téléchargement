(function initOneTrust(){function loadOneTrust(){var cookie=GLOBAL_FUN.getCookie('OptanonAlertBoxClosed');var bannerCookies=$('.banner_cookies');var oneTrustUrl=$('body').attr('data-url-onetrust')||bannerCookies.attr('data-url-onetrust');function hideBanner(){bannerCookies.remove();}
