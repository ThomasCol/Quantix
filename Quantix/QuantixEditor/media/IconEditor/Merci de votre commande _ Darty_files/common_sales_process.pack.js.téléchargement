function decrement(){if(hours===0&&minutes===0&&seconds===0){clearInterval(timer);updateImmediateDeliverySlot(counter);}else if(minutes===0&&seconds===0){minutes=59;seconds=59;hours--;}else if(seconds===0){seconds=59;minutes--;}else{seconds--;}
